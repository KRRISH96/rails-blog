<% if logged_in? %>
<h1 align="center">Welcome to <%= @user.user_name %>'s page</h1>
<div class="row">
  <div class="col-md-4 col-md-offset-4 center">
    <%= gravatar_for @user %>
  </div>
</div>
<h4 align="center"><%= @user.user_name %>'s articles</h4>
<div class="center">
 <%= will_paginate @user_articles %>
</div>
<%= render 'articles/articles', obj: @user_articles %>
<div class="center">
 <%= will_paginate @user_articles %>
</div>
<% else %>
<%= require_user %>
<%end%>